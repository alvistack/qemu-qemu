 qemu-system-gui (<< ${source:Version}),
 qemu-utils (<< ${source:Version}),
 sgabios (<< ${source:Version}),

Package: qemu-system-x86
Architecture: amd64
Description: QEMU full system emulation binaries (x86)
 On x86 host hardware this package also enables KVM kernel virtual
 machine usage on systems which supports it.
Depends:
 ${misc:Depends},
 ${shlibs:Depends},
 ipxe-qemu,
 qemu-system-common (= ${source:Version}),
 seabios,
Recommends:
 ovmf,
 qemu-utils (= ${source:Version}),
Suggests:
 samba,
 vde2,
Provides:
 qemu-kvm,
Breaks:
 qemu-kvm,
Replaces:
 qemu-kvm,

Package: qemu-utils
Architecture: amd64
Description: QEMU utilities
 This package provides QEMU related utilities:
   - qemu-img: QEMU disk image utility
   - qemu-io: QEMU disk exerciser
   - qemu-nbd: QEMU disk network block device server
Depends:
 ${misc:Depends},
 ${shlibs:Depends},
Breaks:
 qemu-system-common (<< 3.1),
Recommends:
 qemu-block-extra (= ${source:Version}),
Suggests:
 debootstrap,

Package: qemu-guest-agent
Architecture: amd64
Description: Guest-side qemu-system agent
 Install this package on a system which is running as guest inside qemu
 virtual machine. It is not used on the host.
Pre-Depends:
 ${misc:Pre-Depends},
Depends:
 ${misc:Depends},
 ${shlibs:Depends},
 lsb-base (>= 3.0-6),
